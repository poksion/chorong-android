<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="net.poksion.chorong.android.samples">

    <application
      android:name="net.poksion.chorong.android.samples.App"
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">

        <activity android:name=".MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>

        <activity android:name=".SampleForAlertDialog">
        </activity>

        <activity android:name=".SampleForFlatCard">
        </activity>

        <activity android:name=".SampleForRouting">
        </activity>

        <activity android:name=".SampleForPersistence">
        </activity>

    </application>

</manifest>